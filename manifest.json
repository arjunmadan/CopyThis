{
	"name": "CopyThis",
	"version": "1.0",
	"manifest_version": 2,
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_title": "CopyThis",
		"default_popup": "popup.html",
		"default_icon": "icon.png"
	},
	"permissions": [
		"contextMenus"
	],
	"icons": {
		"16": "icon-tiny.png",
		"48": "icon.png",
		"128": "icon.png"
	}
}
